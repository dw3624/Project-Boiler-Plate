{
  "name": "synthegra",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:style": "stylelint src/**/*.{css,scss,sass} src/**/*.module.css",
    "eslint:fix": "eslint src --ext .js,jsx,.ts,.tsx --fix",
    "lint:style:fix": "stylelint --fix src/**/*.{css,scss,sass} src/**/*.module.css",
    "prettier:fix": "prettier --check --write src/**/*.{js,jsx,ts,tsx,css,scss,md,mdx}",
    "format": "npm run eslint:fix && npm run lint:style:fix && npm run prettier:fix",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@types/node": "20.1.5",
    "@types/react": "18.2.6",
    "@types/react-dom": "18.2.4",
    "next": "13.4.2",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.0.12",
    "@storybook/addon-interactions": "^7.0.12",
    "@storybook/addon-links": "^7.0.12",
    "@storybook/blocks": "^7.0.12",
    "@storybook/nextjs": "^7.0.12",
    "@storybook/react": "^7.0.12",
    "@storybook/testing-library": "^0.0.14-next.2",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-storybook": "^0.6.12",
    "eslint-plugin-unused-imports": "^2.0.0",
    "prettier": "^2.8.8",
    "scaffdog": "^2.5.0",
    "storybook": "^7.0.12",
    "stylelint": "^15.6.1",
    "stylelint-config-recess-order": "^4.0.0",
    "stylelint-config-recommended-scss": "^11.0.0",
    "typescript": "^5.0.4"
  }
